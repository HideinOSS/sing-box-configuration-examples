{
  "log": {
    "disabled": false,
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "Cloudflare-DoT-v6",
        "type": "tls",
        "server": "one.one.one.one",
        "detour": "anytls-out"
      },
      {
        "tag": "Google-DoT-v6",
        "type": "tls",
        "server": "dns.google",
        "detour": "anytls-out"
      },
      {
        "tag": "AliDNS-DoT-v6",
        "type": "tls",
        "server": "dns.alidns.com",
        "domain_resolver": "Local"
      },
      {
        "tag": "DNSPod-DoT-v6",
        "type": "https",
        "server": "dot.pub",
        "domain_resolver": "Local"
      },
      {
        "tag": "Local",
        "type": "local"
      }
    ],
    "rules": [
      {
        "domain_suffix": [
          "googleapis.cn"
        ],
        "action": "route",
        "server": "Local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "AliDNS-DoT-v6"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        "server": "Cloudflare-DoT-v6",
        "client_subnet": "2001:dd8:1::/48"
      }
    ],
    "final": "",
    "strategy": "prefer_ipv6"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "mtu": 9000,
      "auto_route": true,
      "auto_redirect": true,
      "strict_route": true,
      "stack": "mixed"
    }
  ],
  "outbounds": [
    {
      "type": "anytls",
      "tag": "anytls-out",
      "server": "//your vps ip or domain",
      "server_port": 443,
      "password": "//your anytls password",
      "idle_session_check_interval": "30s",
      "idle_session_timeout": "30s",
      "min_idle_session": 5,
      "tls": {
        "enabled": true,
        "disable_sni": false,
        "server_name": "sns-img-qc.xhscdn.com",
        "insecure": false,
        "utls": {
          "enabled": true,
          "fingerprint": "firefox"
        },
        "reality": {
          "enabled": true,
          "public_key": "//公钥PublicKey，在shell或sing-box所在目录中cmd执行 sing-box generate reality-keypair 生成，取PublicKey。注意一次生成一对，服务端和客户端需要成对对应",
          "short_id": "0123456789abcdef"
        }
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "action": "hijack-dns"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject",
        "method": "default",
        "no_drop": false
      },
      {
        "rule_set": "geosite-category-ads-all",
        "action": "reject",
        "method": "default",
        "no_drop": false
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geoip-cn"
          },
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          }
        ],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/category-ads-all.srs",
        "download_detour": "",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/cn.srs",
        "download_detour": "",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/geolocation-cn.srs",
        "download_detour": "",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "",
        "update_interval": "1d"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geoip/cn.srs",
        "download_detour": "",
        "update_interval": "1d"
      }
    ],
    "auto_detect_interface": true,
    "default_domain_resolver": "Local",
    "final": ""
  }
}
